<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TableView">
        <!--控件这个宽度-->
        <attr name="table_width" format="dimension" />
        <!--控件整个高度-->
        <attr name="table_height" format="dimension" />
        <!-- 背景颜色-->
        <attr name="table_background_color" format="reference|integer" />
        <!--tv1颜色-->
        <attr name="table_tv1_color" format="color" />
        <attr name="table_tv1_size" format="dimension" />
        <!--tv2颜色-->
        <attr name="table_tv2_color" format="color" />
        <attr name="table_tv2_size" format="dimension" />
        <!--tv3颜色-->
        <attr name="table_tv3_color" format="color" />
        <attr name="table_tv3_size" format="dimension" />
        <!-- 中间 背景颜色-->
        <attr name="table_middle_background_color" format="color" />
        <attr name="table_middle_width" format="dimension" />
        <!-- 右边进度条高度-->
        <attr name="table_progress_height" format="dimension" />
        <attr name="table_progress_background_drawable" format="reference" />
        <!-- 右边字体大小-->
        <attr name="table_right_text_size" format="reference|integer" />
        <attr name="table_right_text_color" format="color" />
    </declare-styleable>
</resources>
